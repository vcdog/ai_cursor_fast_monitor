{
  "name": "cursor-usage-monitor",
  "displayName": "Cursor用量监控",
  "description": "监控您的 Cursor AI 使用情况",
  "version": "0.0.17",
  "publisher": "vcdog",
  "icon": "images/icon.png",
  "author": {
    "name": "HiJenkoo"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vcdog/cursor-usage-monitor"
  },
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Other",
    "Visualization",
    "Programming Languages"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "cursor-usage-monitor.checkUsage",
        "title": "检查Cursor用量"
      },
      {
        "command": "cursor-usage-monitor.showUsage",
        "title": "显示Cursor用量详情"
      }
    ],
    "configuration": {
      "title": "Cursor用量监控",
      "properties": {
        "cursor-usage-monitor.cookieString": {
          "type": "string",
          "default": "",
          "description": "Cursor网站的Cookie字符串"
        },
        "cursor-usage-monitor.userId": {
          "type": "string",
          "default": "",
          "description": "Cursor用户ID"
        },
        "cursor-usage-monitor.checkInterval": {
          "type": "number",
          "default": 3600,
          "description": "自动检查间隔（秒）"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "process-icon": "node scripts/process-icon.js",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext js,jsx,ts,tsx",
    "test": "jest"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "@types/node": "^16.11.6",
    "@types/react": "^17.0.34",
    "@types/react-dom": "^17.0.11",
    "@types/vscode": "^1.60.0",
    "babel-loader": "^8.4.1",
    "css-loader": "^6.11.0",
    "eslint": "^8.2.0",
    "jest": "^27.3.1",
    "sharp": "^0.33.5",
    "style-loader": "^3.3.4",
    "terser-webpack-plugin": "^5.3.14",
    "webpack": "^5.98.0",
    "webpack-cli": "^4.10.0"
  },
  "license": "MIT",
  "keywords": [
    "cursor",
    "api",
    "usage",
    "monitor",
    "用量",
    "用量监控",
    "监控",
    "Cursor用量",
    "Cursor监控",
    "Cursor使用情况",
    "Cursor用量监控"
  ],
  "galleryBanner": {
    "color": "#FFFFFF",
    "theme": "light"
  },
  "badges": [
    {
      "url": "https://img.shields.io/badge/Cursor-用量监控-blue",
      "href": "https://marketplace.visualstudio.com/items?itemName=vcdog.cursor-usage-monitor",
      "description": "Cursor用量监控"
    }
  ]
}